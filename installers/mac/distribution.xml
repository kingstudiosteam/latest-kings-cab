<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="1">
    <title>The King's Cab 1.0.0</title>
    <organization>King Studios</organization>
    <domains enable_anywhere="false" enable_currentUserHome="false" enable_localSystem="true"/>
    <options customize="never" require-scripts="true" rootVolumeOnly="true" hostArchitectures="x86_64,arm64"/>
    
    <welcome file="welcome.txt"/>
    <readme file="readme.txt"/>
    
    <pkg-ref id="com.kingstudios.thekingscab"/>
    
    <choices-outline>
        <line choice="default">
            <line choice="com.kingstudios.thekingscab"/>
        </line>
    </choices-outline>
    
    <choice id="default"/>
    <choice id="com.kingstudios.thekingscab" visible="false">
        <pkg-ref id="com.kingstudios.thekingscab"/>
    </choice>
    
    <pkg-ref id="com.kingstudios.thekingscab" version="1.0.0" onConclusion="none">TheKingsCab-Core.pkg</pkg-ref>
</installer-gui-script>
